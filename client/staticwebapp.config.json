{
    "trailingSlash": "auto",
    "routes": [
      {
        "route": "/api/*",
        "methods": ["GET"],
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/api/*",
        "methods": ["PUT", "POST", "PATCH", "DELETE"],
        "allowedRoles": ["contributor"]
      },
      {
        "route": "/login",
        "rewrite": "/.auth/login/github"
      },
      {
        "route": "/logout",
        "redirect": "/.auth/logout"
      }
    ]
  }